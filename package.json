{
  "name": "@changewindows/amaranth",
  "version": "0.5.0",
  "description": "The iconography of ChangeWindows.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "icons": "npm run icons:clean && npm run icons:ts",
    "icons:clean": "svgo -f ./svg -o ./svg",
    "icons:ts": "svg-to-ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "npm run icons && npm run scss && npm run build",
    "scss": "npm run scss:compile && npm run scss:prefix && npm run scss:copy",
    "scss:compile": "node-sass scss -o dist --output-style compressed",
    "scss:copy": "bundle-scss --mask \"scss/amaranth.scss\" --dest \"dist/amaranth.scss\"",
    "scss:prefix": "postcss -u autoprefixer -r dist/*.css --map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/changewindows/amaranth.git"
  },
  "files": [
    "/svg",
    "/dist"
  ],
  "keywords": [
    "icons",
    "svg-icons",
    "iconography"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/changewindows/amaranth/issues"
  },
  "homepage": "https://github.com/changewindows/amaranth#readme",
  "peerDependencies": {
    "react": ">=16.x",
    "clsx": ">=1.x"
  },
  "devDependencies": {
    "@types/react": "^17.0.9",
    "autoprefixer": "^10.2.6",
    "bundle-scss": "^1.5.0",
    "clsx": "^1.1.1",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.34.1",
    "svg-to-ts": "^6.0.1",
    "svgo": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "browserslist": {
    "production": [
      "last 2 versions",
      "not dead",
      "not ie <= 11"
    ]
  }
}
